<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="snakerdemo.dao.user.UserDao">
	<resultMap type="User" id="userResultMapping">
		<id column="ID" property="id" javaType="String"/>
		<result column="name" property="name" javaType="String"/>
	</resultMap>
	
	<sql id="userField"> </sql>
	
	<select id="selectUserById" resultMap="userResultMapping">
	</select>
	
	<select id="selectUserByName" resultMap="userResultMapping">
	</select>
	
	<select id="selectAllUser" resultMap="userResultMapping">
	</select>
	
	<insert id="insertUser" parameterType="User" >
		
	</insert>
	
</mapper>